{
	"name": "nekoaisle-command-menu",
	"displayName": "nekoaisle コマンドメニュー",
	"description": "コマンドを一覧から選んで実行",
	"version": "0.0.2",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": {
		"type": "git",
		"url": "https://github.com/nekoaisle/vsc-extention.git"
	},
	"publisher": "nekoaisle",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nekoaisle.commandMenu",
				"title": "nekoaisle.commandMenu コマンドを一覧から選んで実行"
			},{
				"command": "nekoaisle.multiCommand",
				"title": "nekoaisle.multiCommand 複数コマンド実行"
			}
		],
		"configuration": {
			"type": "object",
			"title": "neko: コマンドメニュー設定",
			"properties": {
				"nekoaisle-commandMenu.menus": {
					"type": "object",
					"default": null,
					"description": "メニューアイテム配列"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
