	// {"key": "ctrl+shift+[Minus]",	"command": "-workbench.action.zoomIn"	},

	// Windowsキーを全て無効化
	{"key": "ctrl+meta+w", "command": "-workbench.action.toggleTabsVisibility"},
	{"key": "meta+enter" , "command": "-workbench.notebook.cell.execute", "when": "inputFocus && notebookEditorFocused"},

	// 定義開始
	{"key": "escape", "command": "kb-macro.abortPlayback", "when": "kb-macro.playing"},
	// {"key": "end", "command": "-cursorEnd", "when": "textInputFocus" },
	{"key": "alt+end", "command": "cursorLineEnd", "when": "textInputFocus" },

	{"key": "f1","command": "-workbench.action.showCommands"},
	{"key": "f1","command": "nekoaisle-command-menu.menu"},
	{"key": "f2","command": "-editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"},
	{"key": "f2","command": "nekoaisle.openHelp", "when": "editorTextFocus"},
	{"key": "f4","command": "nekoaisle-tab-manager.openMenu","when": "editorTextFocus"},
	{"key": "f6" ,"command": "-workbench.action.debug.pause","when": "debugState == 'running'"},
	{"key": "f9" ,"command": "-sortLines.sortLines","when": "editorTextFocus"},
	{"key": "f9" ,"command": "-editor.debug.action.toggleBreakpoint","when": "editorTextFocus"},
	{"key": "f9" ,"command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly"},
	{"key": "f9" ,"command": "workbench.action.debug.continue","when": "debugState == 'stopped'"},
	{"key": "f10","command": "nekoaisle.toggleTab", "when": "editorTextFocus && debugState != 'stopped'"},
	// {"key": "f10","command": "workbench.action.openNextRecentlyUsedEditorInGroup", "when": "editorTextFocus"},

	{"key": "shift+f1"    ,"command": "nekoaisle-multiClipboard.menu", "when": "editorTextFocus && !editorReadonly"},
	{"key": "shift+f2"    ,"command": "editor.action.rename","when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"},
	{"key": "shift+f5"    , "command": "workbench.action.debug.stop","when": "debugState == 'running'"},
	{"key": "shift+f9"    ,"command": "-editor.debug.action.toggleInlineBreakpoint","when": "editorTextFocus"},
	{"key": "shift+enter" ,"command": "nekoaisle.selectWord","when": "editorTextFocus"},
	{"key": "shift+insert","command": "-editor.action.selectionClipboardPaste"},
	{"key": "shift+insert","command": "editor.action.clipboardPasteAction", "when": "editorTextFocus && !editorReadonly"},

	{"key": "ctrl+enter","command": "-editor.action.insertLineAfter","when": "editorTextFocus && !editorReadonly"},
	{"key": "ctrl+enter","command": "removeSecondaryCursors", "when": "editorHasMultipleSelections && editorTextFocus"},
	{"key": "ctrl+up"   ,"command": "nekoaisle.delCursor", "when": "editorTextFocus"},
	{"key": "ctrl+down" ,"command": "nekoaisle.addCursor", "when": "editorTextFocus"},
	{"key": "ctrl+[BracketLeft]", "command": "nekoaisle.encloseInGraveAccent", "when": "editorTextFocus && !editorReadonly"},
