{
	"name": "nekoaisle-exec",
	"displayName": "Nekoaisle Execute shell command",
	"description": "任意のシェルコマンドを実行",
	"version": "0.0.1",
	"icon": "logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/nekoaisle/vsc-extention.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nekoaisle.exec",
				"title": "nekoaisle.exec 指定シェルコマンドを実行"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
