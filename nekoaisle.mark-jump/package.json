{
    "name": "nekoaisle-mark-jump",
    "displayName": "Nekoaisle Mark Jump",
    "description": "カーソル位置をマークしマークした位置にジャンプします",
    "version": "0.1.2",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": "https://github.com/nekoaisle/vsc-extention.git",
	"publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "nekoaisle.markjumpMark",
                "title": "Nekoaisle mark-jump マーク"
            },
            {
                "command": "nekoaisle.markjumpJump",
                "title": "Nekoaisle mark-jump ジャンプ"
            },
            {
                "command": "nekoaisle.markjumpReturn",
                "title": "Nekoaisle mark-jump 最後のカーソル位置に"
            }
        ],
		"configuration": {
			"type": "object",
			"title": "neko: ジャンプ閾値(行数)",
			"properties": {
				"nekoaisle-mark-jump.jump-threshold": {
					"type": "string",
					"default": "10",
					"description": "ジャンプと記録する行数を指定します。"
				}
			}
		}
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "package": "npx vsce package && mv *.vsix ../vsix && ls -ahltr ../vsix"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^6.2.1",
        "typescript": "^2.0.3",
        "vscode": "^1.1.36"
    }
}
