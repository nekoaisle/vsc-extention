{
  "name": "open-filer",
  "displayName": "Open filer",
  "description": "現在のファイルが格納されているディレクトリをファイラーで開く",
  "version": "0.0.2",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": {
		"type": "git",
		"url": "https://github.com/nekoaisle/vsc-extention.git"
	},
  "publisher": "nekoaisle",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nekoaisle.openFiler"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "nekoaisle.openFiler",
        "title": "nekoaisle.openFiler ファイラーを開く"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "neko: ファイラーを開く",
      "properties": {
        "nekoaisle-openFiler.filer": {
          "type": "string",
          "default": "nautilus",
          "description": "ファイラーのファイル名及びコマンドラインオプションを指定してください。"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^6.2.1",
    "typescript": "^2.0.3",
    "vscode": "^1.1.36"
  }
}
