{
    "name": "nekoaisle-select-word",
    "displayName": "Nekoaisle Select word",
    "description": "単語選択, 矩形選択",
    "version": "0.0.2",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": {
		"type": "git",
		"url": "https://github.com/nekoaisle/vsc-extention.git"
	},
    "publisher": "nekoaisle",
    "engines": {
		"vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "nekoaisle.selectWord",
                "title": "単語選択"
            },
            {
                "command": "nekoaisle.addCursor",
                "title": "次の行の同じ桁にカーソルを追加"
            },
            {
                "command": "nekoaisle.delCursor",
                "title": "一番下のカーソルを削除"
            }
        ]
    },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"package": "npx vsce package && mv *.vsix ../vsix && ls -ahltr ../vsix"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	},
	"dependencies": {
		"json-cyclic": "^1.0.2"
	}
}
