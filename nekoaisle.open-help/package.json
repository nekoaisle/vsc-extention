{
	"name": "open-help",
	"displayName": "Open help",
	"description": "カーソル位置の単語でマニュアルなどを開く",
	"version": "0.0.2",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": "https://github.com/nekoaisle/vsc",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nekoaisle.openHelp"
	],
	"main": "./out/extension.js",
	"contributes": {
