{
	"name": "nekoaisle-calc",
	"displayName": "nekoaisle-calc",
	"description": "コマンドライン電卓",
	"version": "0.0.1",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": "https://github.com/nekoaisle/vsc",
	"publisher": "nekoaisle",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:nekoaisle.calc"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "nekoaisle.calc",
				"title": "nekoaisle.calc コマンドライン電卓"
			}
		],
		"configuration": {
			"type": "object",
			"title": "neko: 電卓",
			"properties": {
				"nekoaisle-calc.format": {
					"type": "string",
					"default": "%f",
					"description": "書式指定文字列"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/sprintf-js": "^1.1.2",
		"sprintf-js": "^1.1.2"
	}
}
