{
    "name": "cpss-log-highlight",
    "displayName": "CPSS log Highlight",
    "description": "Highlight the syntax of the CPSS log file",
    "version": "0.0.1",
	"icon": "logo.png",
	"license": "MIT License",
	"repository": {
		"type": "git",
		"url": "https://github.com/nekoaisle/vsc-extention.git"
	},
	"publisher": "nekoaisle",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cpsslog",
                "aliases": [
                    "CPSS-log",
                    "cpsslog"
                ],
                "extensions": [
                    ".log"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cpsslog",
                "scopeName": "text.cpss-log",
                "path": "./syntaxes/cpsslog.tmLanguage.json"
            }
        ]
    }
}
